<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="seq_trip_number" model="ir.sequence">
        <field name="name">Trip Number</field>
        <field name="code">hr.travel</field>
        <field name="prefix"> - </field>
        <field name="padding">3</field>
        <field name="company_id" eval="False" />
    </record>

    <record id="seq_awarded_days" model="ir.sequence">
        <field name="name">Awarded Days</field>
        <field name="code">hr.awarded.days</field>
        <field name="prefix"> - </field>
        <field name="padding">3</field>
        <field name="company_id" eval="False" />
    </record>

    <record id="seq_hr_appraisal" model="ir.sequence">
        <field name="name">Appraisal</field>
        <field name="code">hr.appraisal</field>
        <field name="prefix">ER - </field>
        <field name="padding">3</field>
        <field name="company_id" eval="False" />
    </record>

    <record id="asset_sequence_id" model="ir.sequence">
            <field name="name">Asset Management</field>
            <field name="code">asset.management</field>
            <field name="prefix">Asset - </field>
            <field name="padding">3</field>
        </record>
</odoo>